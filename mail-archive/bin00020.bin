msgid "KNode "
msgstr "KNode"

#: main.cpp:71
msgid ""
"KNode is a newsreader for KDE\n"
"\n"
msgstr ""
"KNode è un newsreader for KDE\n"
"\n"

#: main.cpp:72
msgid ""
"Usage: knode [-display xxx] [-h|-v|-c xxx]\n"
"\n"
msgstr ""
"Uso: knode [-display xxx] [-h|-v|-c xxx]\n"
"\n"

#: main.cpp:73
msgid "Options:\n"
msgstr "Opzioni:\n"

#: main.cpp:74
msgid "   -display xxx     sets the X display (default is $DISPLAY)\n"
msgstr "   -display xxx     imposta l'X display (default è $DISPLAY)\n"

#: main.cpp:75
msgid "Commands:\n"
msgstr "Comandi:\n"

#: main.cpp:76
msgid "   -h / --help             display this help and exit\n"
msgstr "   -h / --help             mostra questo aiuto ed esce\n"

#: main.cpp:77
msgid "   -v / --version          display version information and exit\n"
msgstr "   -v / --version          visualizza informazioni sulla versione ed esce\n"

#: main.cpp:78
msgid "   -c xxx / --convert xxx  convert the database from the format\n"
msgstr "   -c xxx / --convert xxx  converte il database dal formato\n"

#: main.cpp:79
msgid "                           of version xxx into the currently used\n"
msgstr "                           di versione xxx usato attualmente\n"

#: main.cpp:80
msgid ""
"                           format. More information in README.\n"
"\n"
msgstr ""
"                           formato. Altre informazioni nel README.\n"
"\n"

